<div class="row justify-content-space-between blog-card">
    <p id="profile" class="col-12 col-md-12 col-lg-12 text-center">Welcome back!</p>
    {{#if posts.length}}

    {{#each posts as |post|}}
    <div class="card border-0 shadow p-3 mb-5 bg-white rounded col-12 col-md-12 col-lg-12" data-number={{post.id}}
      
      <div class="card-body">
        <h5 class="card-title text-capitalize text-center">{{post.title}}</h5>
      </div>
    </div>
    {{/each}}
    {{/if}}
  </div>
</div>